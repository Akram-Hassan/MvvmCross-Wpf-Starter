<views:MvxWpfView 
            x:Class="Kimo.MwwmCross.Wpf.Starter.Gui.Views.Page1"
            xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Kimo.MwwmCross.Wpf.Starter.Gui.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel VerticalAlignment="Center">
        <TextBlock Text="{Binding PageName}" Height="100" Width="200" Background="AliceBlue" Margin="20"></TextBlock>
        <Button Content="Go tio page 2" Height="50" Width="200" Command="{Binding GoToPage1Command}"></Button>
    </StackPanel>
</views:MvxWpfView>
